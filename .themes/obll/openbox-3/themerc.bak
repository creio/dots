# Theme name : sendiki-obtgen 
# Generate from obtgen

# Section: menu
menu.border.width:                      10
menu.border.color:                      #d9e1e8
menu.separator.color:                   #d9e1e8
menu.*.bg:                              flat
menu.*.bg.color:                        #d9e1e8
menu.*.text.color:                      #16191f
menu.*.text.justify:                    center 
menu.*.disabled.text.color:             #d9e1e8
menu.*.active.text.color:               #d9e1e8
menu.*.active.bg.color:                 #16191f

# Section: osd
osd.border.width:                       0
osd.border.color:                       #d9e1e8
osd.bg:                                 flat
osd.bg.color:                           #d9e1e8
osd.label.bg:                           flat solid
osd.label.bg.color:                     #d9e1e8
osd.label.text.color:                   #16191f
osd.hilight.bg:                         flat solid
osd.hilight.bg.color:                   #8897F4
osd.unhilight.bg:                       flat
osd.unhilight.bg.color:                 #414458
osd.button.unpressed.bg:                flat
osd.button.unpressed.bg.color:          #414458
osd.button.unpressed.*.border.color:    #d9e1e8
osd.button.pressed.bg:                  flat
osd.button.pressed.bg.color:            #414458
osd.button.pressed.*.border.color:      #d9e1e8
osd.button.focused.bg:                  flat  
osd.button.focused.bg.color:            #d9e1e8
osd.button.focused.*.border.color:      #d9e1e8
osd.button.focused.box.color:           #8897F4

# Section: window
window.*.bg:                            flat parentrelative
window.*.*.bg:                          flat parentrelative
window.*.*.*.bg:                        flat parentrelative
window.*.text.justify:                  center
window.active.button.*.*.image.color:   #d9e1e8
window.active.label.text.color:         #d9e1e8
window.active.title.bg.color:           #67b2ef
window.active.title.separator.color:    #67b2ef
window.active.border.color:             #67b2ef
window.inactive.button.*.*.image.color: #d9e1e8
window.inactive.label.text.color:       #d9e1e8
window.inactive.title.bg.color:         #414458
window.inactive.title.separator.color:  #414458
window.inactive.border.color:           #67b2ef

window.*.button.*.image.color:           #d9e1e8
window.*.label.text.color:               #d9e1e8
window.*.title.bg.color:                 #16191f
window.*.title.separator.color:          #16191f
window.*.border.color:                   #16191f
window.*.handle.bg.color:                #d9e1e8

# Section: fonts
window.active.label.text.font:          shadow=n
window.inactive.label.text.font:        shadow=n
menu.items.font:                        shadow=n
menu.title.text.font:                   shadow=n

# Section: misc
border.width:                           0
padding.width:                          10
padding.height:                         7
window.client.padding.height:           0
window.client.padding.width:            0
window.label.text.justify:              left
