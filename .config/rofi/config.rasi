configuration {
  theme: "default.rasi";
  show-icons: true;
  icon-theme: "Qogir-dark";
  font: "ClearSansMedium 10";
  terminal: "urxvt";
  modi: "drun,run,window";
  ssh-client: "ssh";
  ssh-command: "{terminal} -e {ssh-client} {host} [-p {port}]";
  run-command: "{cmd}";
  run-shell-command: "{terminal} -e {cmd}";
  window-command: "wmctrl -i -R {window}";
  sidebar-mode: true;
}
